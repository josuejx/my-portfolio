---
import TimeLineExperience from "../../components/TimeLineExperience.astro";
---

<section id="studies" class="flex flex-col">
    <div>
      <h2 class="text-2xl font-bold mb-10">Estudios</h2>
    </div>
    <div class="ml-5">
      <ol class="relative border-s border-gray-200 dark:border-gray-700">
        <TimeLineExperience
          titulo="Universidad Internacional de La Rioja (UNIR)"
          icono="M8.25 3v1.5M4.5 8.25H3m18 0h-1.5M4.5 12H3m18 0h-1.5m-15 3.75H3m18 0h-1.5M8.25 19.5V21M12 3v1.5m0 15V21m3.75-18v1.5m0 15V21m-9-1.5h10.5a2.25 2.25 0 0 0 2.25-2.25V6.75a2.25 2.25 0 0 0-2.25-2.25H6.75A2.25 2.25 0 0 0 4.5 6.75v10.5a2.25 2.25 0 0 0 2.25 2.25Zm.75-12h9v9h-9v-9Z"
          fecha="oct. 2023 - jul. 2024"
        >
          <p
            class="mb-4 text-base font-normal text-gray-500 dark:text-gray-400"
          >
            Máster Oficial en Inteligencia Artificial
          </p>
        </TimeLineExperience>

        <TimeLineExperience
